<p id="notice"><%= notice %></p>

<h1>Listing Polis</h1>

<table>
  <thead>
    <tr>
      <th>Nama</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @polis.each do |poli| %>
      <tr>
        <td><%= poli.nama %></td>
        <td><%= link_to 'Show', poli %></td>
        <td><%= link_to 'Edit', edit_poli_path(poli) %></td>
        <td><%= link_to 'Destroy', poli, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>



<%= link_to 'New Poli', new_poli_path %>
