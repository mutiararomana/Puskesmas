
<table class="table">
  <thead>
    <tr>
      <th>Tanggal</th>
      <th>Obat</th>
      <th>Jumlah ambil</th>
      <th>Pemakaian</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
      <% @total_reseps.each do |t| %>
      
      <tr> 
          <td><%= t.obat.nama %></td>
          <td><%= t.total %></td>
          <% @reseps.each do |r| %>
          <td><%= @r.created_at.to_date %></td>
          <td><%= @r.pemakaian %></td>
          <% end %>
        <td><%= link_to 'Show', "#" %></td>
          
      </tr>
      
    <% end %>
  </tbody>
</table>