<p id="notice"><%= notice %></p>

<h1>Listing Obat Masuks</h1>

<table>
  <thead>
    <tr>
      <th>Obat</th>
      <th>Jumlah masuk</th>
      <th>Penerimaan</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @obat_masuks.each do |obat_masuk| %>
      <tr>
        <td><%= obat_masuk.obat.nama %></td>
        <td><%= obat_masuk.jumlah_masuk %></td>
        <td><%= obat_masuk.penerimaan %></td>
        <td><%= link_to 'Show', obat_masuk %></td>
        <td><%= link_to 'Edit', edit_obat_masuk_path(obat_masuk) %></td>
        <td><%= link_to 'Destroy', obat_masuk, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Obat masuk', new_obat_masuk_path %>
