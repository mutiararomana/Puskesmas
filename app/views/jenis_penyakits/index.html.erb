<p id="notice"><%= notice %></p>

<h1>Listing Jenis Penyakits</h1>

<table>
  <thead>
    <tr>
      <th>Nama</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @jenis_penyakits.each do |jenis_penyakit| %>
      <tr>
        <td><%= jenis_penyakit.nama %></td>
        <td><%= link_to 'Show', jenis_penyakit %></td>
        <td><%= link_to 'Edit', edit_jenis_penyakit_path(jenis_penyakit) %></td>
        <td><%= link_to 'Destroy', jenis_penyakit, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Jenis penyakit', new_jenis_penyakit_path %>
